{
    "name": "drmonty/datatables-tabletools",
    "description": "TableTools Plugin for DataTables",
    "type": "component",
    "homepage": "http://www.datatables.net/",
    "author": {
        "name": "Allan Jardine",
        "url": "http://sprymedia.co.uk"
    },
    "require": {
        "robloach/component-installer": "*"
    },
    "require": {
        "components/jquery": ">=1.7"
    },
    "extra": {
        "component": {
            "scripts": [
                "js/dataTables.tableTools.js"
            ],
            "files": [
                "js/dataTables.tableTools.min.js",
                "css/dataTables.tableTools.min.css",
                "images/*.png",
                "swf/*.swf"
            ],
            "shim": {
                "deps": ["jquery"]
            }
        }
    }
}